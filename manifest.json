{
  "manifest_version": 3,
  "name": "Note Taker",
  "version": "1.0",
  "description": "A simple note-taking extension",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Note Taker"
  }
}
